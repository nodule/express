{
  "express.app": {
    "fn": "output = { app: express.express() }\n",
    "ports": {
      "output": {
        "app": {
          "type": "object"
        }
      }
    },
    "require": {
      "express": "latest"
    },
    "description": "express app",
    "name": "express.app"
  },
  "express.get": {
    "fn": "output = [input.app, 'get', input.path]\n",
    "ports": {
      "input": {
        "app": {
          "type": "object"
        },
        "path": {
          "type": "string",
          "default": "/"
        }
      },
      "output": {
        "req": {
          "type": "object"
        },
        "res": {
          "type": "object"
        },
        "next": {
          "type": "object"
        }
      }
    },
    "description": "express get",
    "name": "express.get"
  },
  "express.listen": {
    "fn": "output = { output: input.app.listen(input.port) }\n",
    "ports": {
      "input": {
        "app": {
          "type": "object"
        },
        "port": {
          "type": "string"
        }
      },
      "output": {
        "output": {
          "type": "object"
        }
      }
    },
    "description": "express listen",
    "name": "express.listen"
  },
  "express.post": {
    "fn": "output = [input.app, 'post', input.path]\n",
    "ports": {
      "input": {
        "app": {
          "type": "object"
        },
        "path": {
          "type": "string"
        }
      },
      "output": {
        "output": {
          "type": "object"
        }
      }
    },
    "description": "express post",
    "name": "express.post"
  },
  "express.send": {
    "fn": "\n//input.req\n//input.next\n\ninput.res.send(input.body)\n\n// todo: do something with the success port\n",
    "ports": {
      "input": {
        "body": {
          "type": "object"
        }
      },
      "output": {}
    },
    "description": "express send",
    "name": "express.send"
  }
}