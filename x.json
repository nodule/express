{
  "name": "express",
  "description": "Express for ChiÏ‡",
  "version": "0.0.1",
  "licences": [
    {
      "type": "MIT",
      "url": "https://github.com/psichi/chix/raw/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/psichi/chix-express.git"
  },
  "dependencies": {
    "express": "latest"
  },
  "nodes": [
    {
      "name": "app",
      "description": "express app",
      "require": {
        "express": "latest"
      },
      "ports": {
        "input": {
          "dummy": {
            "type": "null"
          }
        },
        "output": {
          "app": {
            "type": "function"
          },
          "dummy": {
            "type": "null"
          }
        }
      },
      "ns": "express"
    },
    {
      "name": "get",
      "description": "express get",
      "ports": {
        "input": {
          "app": {
            "type": "function"
          },
          "path": {
            "type": "string",
            "default": "/"
          }
        },
        "output": {
          "req": {
            "type": "object"
          },
          "res": {
            "type": "object"
          },
          "next": {
            "type": "function"
          }
        }
      },
      "ns": "express"
    },
    {
      "name": "listen",
      "description": "express listen",
      "ports": {
        "input": {
          "app": {
            "type": "function"
          },
          "port": {
            "type": "string"
          },
          "dummy": {
            "type": "null"
          }
        },
        "output": {
          "output": {
            "type": "object"
          }
        }
      },
      "ns": "express"
    },
    {
      "name": "post",
      "description": "express post",
      "ports": {
        "input": {
          "app": {
            "type": "function"
          },
          "path": {
            "type": "string"
          }
        },
        "output": {
          "output": {
            "type": "object"
          }
        }
      },
      "ns": "express"
    },
    {
      "name": "send",
      "description": "express send",
      "ports": {
        "input": {
          "req": {
            "type": "object"
          },
          "res": {
            "type": "object"
          },
          "next": {
            "type": "function"
          }
        },
        "output": {
          "dummy": {
            "type": "null"
          }
        }
      },
      "ns": "express"
    }
  ],
  "twigs": {
    "ns": "express",
    "get": {
      "name": "get",
      "description": "ExpressJS get",
      "nodes": [
        {
          "ns": "express",
          "name": "app"
        },
        {
          "ns": "express",
          "name": "get",
          "context": {
            "path": "/"
          }
        },
        {
          "ns": "express",
          "name": "send"
        },
        {
          "ns": "express",
          "name": "listen",
          "context": {
            "port": "2376"
          }
        }
      ],
      "links": [
        {
          "source": 0,
          "target": 1,
          "out": "app",
          "in": "app"
        },
        {
          "source": 0,
          "target": 3,
          "out": "app",
          "in": "app"
        },
        {
          "source": 1,
          "target": 2,
          "out": "req",
          "in": "req"
        },
        {
          "source": 1,
          "target": 2,
          "out": "res",
          "in": "res"
        },
        {
          "source": 1,
          "target": 2,
          "out": "next",
          "in": "next"
        },
        {
          "source": 2,
          "target": 3,
          "out": "dummy",
          "in": "dummy"
        }
      ]
    }
  }
}